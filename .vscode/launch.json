    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch EPMCreateJIT",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMCreateJIT.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -log"]            
        },
        {
            "name": "PowerShell: Launch EPMHealthCheckGathering",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMHealthCheckGathering.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Monitor(cyberark_16)\""]
        },
        {
            "name": "PowerShell: Launch EPMApplicationUpdateExample",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMApplicationUpdateExample.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Monitor(cyberark_16)\" -log"]
        },
        {
            "name": "EPMDuplicateAgentConf",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMDuplicateAgentConf.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Elevate(cyberark_16)\" -copy -agentPolicyName \"Ubuntu-4\" -destCompName \"Ubuntu2004-1\""]
            //"args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Elevate(cyberark_16)\" -delete -agentPolicyName \"Ubuntu2004-1\""]
        }, 
        {
            "name": "EPMPolicyValidator",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMPolicyValidator.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Monitor(cyberark_16)\" -ScanPolicies \"all\""]
        },
        {
            "name": "EPMMoveComputer",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMMoveComputer.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Monitor(cyberark_16)\" -computerList \"computerlist.txt\" -destSetName \"[Giulio] Elevate(cyberark_16)\" -log"]
        },           
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu"]
        }
    ]
}