    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch EPMCreateJIT",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMCreateJIT.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -log"]            
        },
        {
            "name": "PowerShell: Launch EPMHealthCheckTool",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMHealthCheckTool.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Elevate(cyberark_16)\""]
        },
        {
            "name": "PowerShell: Launch EPMApplicationUpdateExample",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMApplicationUpdateExample.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Monitor(cyberark_16)\" -log"]
        },
        {
            "name": "PowerShell: Launch EPMDuplicateAgentConf",
            "type": "PowerShell",
            "request": "launch",
            "script": "./EPMDuplicateAgentConf.ps1",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu -set \"[Giulio] Elevate(cyberark_16)\" -agentPolicyName \"ubuntu-2204-1\" -destCompName \"Ubuntu2004-1\""]
        },     
        {
            "name": "PowerShell: Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "args": ["-username giulio.compagnone@cyber-ark.com -tenant eu"]
        }
    ]
}